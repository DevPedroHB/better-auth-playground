{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [".env*"],
  "tasks": {
    "//#format-and-lint": {},
    "//#format-and-lint:fix": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "start:dev": {
      "cache": false,
      "persistent": true
    }
  },
  "ui": "tui"
}
